<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd">

	<util:list id="converters">
		<!-- 
			Add your own converters using entries of the following form: 
			<bean class="converters.Class1ToClass2" /> 
		-->
		<!-- ALVARO -->
		<bean class="converters.StringToCurriculaConverter"/>
		<bean class="converters.CurriculaToStringConverter"/>
		<bean class="converters.StringToJobApplicationConverter"/>
		<bean class="converters.JobApplicationToStringConverter"/>
		<bean class="converters.StringToHostConverter"/>
		<bean class="converters.HostToStringConverter"/>
		<!-- ALVARO -->
		
		<!-- ANTONIO -->
		<bean class="converters.StringToCleanerConverter" />		
		<!-- ANTONIO -->
		<bean class="converters.StringToMailboxConverter" />		
		<bean class="converters.StringToTagConverter" />
		
		<!-- Ferrete -->
		<bean class="converters.StringToCustomerConverter" />		
		<bean class="converters.CustomerToStringConverter" />
		<bean class="converters.StringToAccomodationConverter" />		
		<bean class="converters.AccomodationToStringConverter" />
		<bean class="converters.StringToWarrantyConverter" />		
		<bean class="converters.StringToTransportConverter" />		
		<bean class="converters.TransportToStringConverter" />
		<bean class="converters.StringToWarrantyConverter" />		
		<bean class="converters.WarrantyToStringConverter" />
		<bean class="converters.HostToStringConverter" />
		<bean class="converters.StringToHostConverter" />	
		<bean class="converters.TransporterToStringConverter" />
		<bean class="converters.StringToTransporterConverter" />	
		<bean class="converters.TravelAgencyToStringConverter" />
		<bean class="converters.StringToTravelAgencyConverter" />
		<!-- Ferrete -->
	
	</util:list>
			
	
</beans>